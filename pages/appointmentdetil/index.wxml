
<block wx:for="{{applist}}" wx:key="id">
                  <view  bindtap="banBtn"data-id='{{item.itemid}}'style="background-color:white;width:100%;height:260rpx;position: relative;margin-bottom:20rpx;margin-top:20rpx;">
                       <image class="yy-pic" src="{{item.picurl}}"/>
                        <view class="yy-title">{{item.title}}</view>
                        <view class="yy-price">数量:{{item.num}}</view>
                        <view class="yy-time">预约时间：{{item.servicetime}}</view>
                        <view class="zt">{{item.paystatus==0?"未支付":"已支付"}}</view>
                    </view> 
</block>
 <button class="kf_button" open-type="contact" session-from="weapp">
    <image class="kf_image" src="../../image/kf_icon_default_01@2x.png"></image>
</button>