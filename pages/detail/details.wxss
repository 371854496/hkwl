/* pages/detail/details.wxss */
@import '../common/lib/weui.wxss';
@import "/wxParse/wxParse.wxss";

.swiper-ad {
  height: 500rpx;
  width: 100%;
}

.swiper-image {
  height: 446rpx;
  width: 750rpx;
}

.on {
  color: black;
  border-bottom: 3rpx solid rgb(240,196,211);
}

.swiper-box {
  display: block;
  height: 100%;
  width: 100%;
  overflow: hidden;
}

.pro-detail-title {
  background-color: white;
  margin-left: 15px;
  margin-right: 15px;
  margin-top: 10px;
  font-size: 16px;
}

.pro-detail-price {
  background-color: white;
  color: #996B7A;
  margin-left: 16px;
  padding-top:5px;
  padding-bottom:5px;
}

.pro-detail-foot {
  border-top: 1px solid #eee;
  background-color: white;
  width: 100%;
  height: 50px;
  position: fixed;
  bottom: 0px;
  font-size: 14px;
}

.placeholder {
  text-align: center;
  height: 100%;
  line-height: 50px;
}

.pro-buy {
  background-color: rgb(240,196,211);
  color: white;
}

.pro-addcart {
  background-color: #eee;
}

/*使屏幕变暗  */

.commodity_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.2;
  overflow: hidden;
  z-index: 1000;
  color: #fff;
}

/*对话框 */

.commodity_attr_box {
  height: 450px;
  width: 100%;
  overflow: hidden;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 2000;
  background: #fff;
}

.weui-panel {
  background-color: #fff;
  margin-top: 0px;
  position: relative;
  overflow: hidden;
}

.weui-panel:first-child {
  margin-top: 0;
}

.weui-panel:before {
  top: 0;
  border-top: 0rpx solid #e5e5e5;
}

.weui-panel:after, .weui-panel:before {
  content: " ";
  position: absolute;
  left: 0;
  right: 0;
  height: 1px;
  color: #e5e5e5;
}

.weui-panel:after {
  bottom: 0;
  border-bottom: 1rpx solid #e5e5e5;
}

.item-attr-pictrue {
  width: 80px;
  height: 80px;
